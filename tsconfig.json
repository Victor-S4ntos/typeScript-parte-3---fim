{
    "compilerOptions": {
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "undici-types": ["node_modules/undici-types"]
    },
    "outDir": "dist/js",
    "target": "ES6",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "removeComments": true,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "sourceMap": true
  },
  "include": ["app/**/*", "js"]
}